{
    "name": "Browsing History Survey",
    "version": "0.2",
    "description": "Surveying the browsing habits of users to explore the effects of algorithmic bias",
    "permissions": [
        "history",
        "tabs",
        "storage",
        "nativeMessaging",
        "contextMenus"
    ],
    "background": {
        "scripts": ["init.js", "timedQueries.js", "background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "historySurvey.html"
    },
    "manifest_version": 2
}