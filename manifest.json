{
    "name": "Browsing History Survey",
    "version": "0.2",
    "description": "Surveying the browsing habits of users to explore the effects of algorithmic bias",
    "permissions": [
        "history",
        "tabs",
        "storage",
        "nativeMessaging",
        "http://127.0.0.1:2812/*"
    ],
    "background": {
        "scripts": ["init.js", "timedQueries.js"],
        "persistent": true
    },
    "browser_action": {
        "default_popup": "historySurvey.html"
    },
    "manifest_version": 2
}